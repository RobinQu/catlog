    "couchdb",
    "console"
  ],
  "dependencies": {
    "colors": "^1.1.2",
    "dateformat": "^1.0.12",
    "eventemitter3": "^1.1.1",
    "ioredis": "^1.14.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "plugin-party": "~0.1.0",
    "rcfiles": "^1.0.1"
  },
  "author": "RobinQu",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.11.6",
    "eslint": "^1.0.0",
    "istanbul": "^0.4.2",
    "minami": "^1.1.1",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "^1.0.0",
    "sinon": "^1.15.4"
  }
